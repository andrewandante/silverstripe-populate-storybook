{
    "name": "silverstripe/populate-storybook",
    "description": "Wrapper on Populate to render HTML for Storybook stories.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "populate",
        "storybook"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "dnadesign/silverstripe-populate": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "AndrewAndante\\PopulateStoryBook\\": "src/",
            "AndrewAndante\\PopulateStoryBook\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
